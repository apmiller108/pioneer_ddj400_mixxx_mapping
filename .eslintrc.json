{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
    },
    "globals": {
        "midi": "readonly",
        "engine": "readonly",
        "_": "readonly",
        "script": "readonly"
    }
}
